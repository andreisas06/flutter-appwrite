# Flavor options: development, staging, production
FLAVOR=development

# Title of the app as it will be displayed on the home screen
APP_TITLE=[DEV] Base App
# Unique identifier for the app
APP_ID=com.baseflow.base_app

# Base URL for the API
BASE_URL=https://randomuser.me/api/

# URL for the authentication service
AUTH_URL=https://example.com/auth

# Default username and password, used to prefil the login form when running in debug mode
DEFAULT_USERNAME=dev@baseflow.com
DEFAULT_PASSWORD=password